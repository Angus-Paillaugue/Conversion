#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Dev/Conversion/node_modules/.pnpm/javascript-obfuscator@4.1.0/node_modules/javascript-obfuscator/bin/node_modules:/mnt/d/Dev/Conversion/node_modules/.pnpm/javascript-obfuscator@4.1.0/node_modules/javascript-obfuscator/node_modules:/mnt/d/Dev/Conversion/node_modules/.pnpm/javascript-obfuscator@4.1.0/node_modules:/mnt/d/Dev/Conversion/node_modules/.pnpm/node_modules:/mnt/d/Dev/Conversion/node_modules:/mnt/d/Dev/node_modules:/mnt/d/node_modules:/mnt/d/Dev/conversion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Dev/Conversion/node_modules/.pnpm/javascript-obfuscator@4.1.0/node_modules/javascript-obfuscator/bin/node_modules:/mnt/d/Dev/Conversion/node_modules/.pnpm/javascript-obfuscator@4.1.0/node_modules/javascript-obfuscator/node_modules:/mnt/d/Dev/Conversion/node_modules/.pnpm/javascript-obfuscator@4.1.0/node_modules:/mnt/d/Dev/Conversion/node_modules/.pnpm/node_modules:/mnt/d/Dev/Conversion/node_modules:/mnt/d/Dev/node_modules:/mnt/d/node_modules:/mnt/d/Dev/conversion/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/javascript-obfuscator" "$@"
else
  exec node  "$basedir/../../bin/javascript-obfuscator" "$@"
fi
